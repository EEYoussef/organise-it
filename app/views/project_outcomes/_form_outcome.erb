<%=form_with model: [@project,@project_outcome], local: true do |f|%>
  
  <div class='form-group mb-3'> 
  <h5>
    <%=f.label :title, class: "form-label"%>
    </h5>
    <%=f.text_field :title%>
  </div>
   <div class='form-group mb-3'> 
    <%=f.label :details, class: "form-label"%>
    <%=f.text_area :details, class: "form-control"%>
  </div>
  
  

<div class='actions'> 
   <%=f.submit%>
  </div>
<%end%>